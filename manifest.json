{
  "manifest_version": 2,

  "name": "ApplyFilters",
  "description": "Apply effects and filters to page",
  "version": "1.0",

  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
